<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.femass.controller.MainController">
   <center>
      <HBox prefHeight="100.0" prefWidth="282.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button" onAction="#handleButtonAction" text="Clientes" />
            <Label fx:id="label" minHeight="16" minWidth="69">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="BtnConta" layoutX="30.0" layoutY="30.0" onAction="#handleBtnContaAction" text="Conta Corrente" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </center>
</BorderPane>
